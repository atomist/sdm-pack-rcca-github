{
  "name": "@atomist/sdm-pack-rcca-github",
  "version": "0.2.0",
  "description": "An Atomist extension pack to manage GitHub resources like Webhooks",
  "author": {
    "name": "Atomist",
    "email": "support@atomist.com",
    "url": "https://atomist.com/"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/atomist/sdm-pack-rcca-github#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist/sdm-pack-rcca-github.git"
  },
  "bugs": {
    "url": "https://github.com/atomist/sdm-pack-rcca-github/issues"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "rcca",
    "github"
  ],
  "main": "atomist.config.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@atomist/slack-messages": "^1.1.0",
    "@octokit/plugin-throttling": "^2.6.0",
    "@octokit/rest": "^16.27.0",
    "@types/fs-extra": "^7.0.0",
    "fs-extra": "^8.0.1",
    "lodash": "^4.17.11",
    "minimatch": "^3.0.4"
  },
  "peerDependencies": {
    "@atomist/automation-client": ">=1.6.0",
    "@atomist/sdm": ">=1.6.0",
    "@atomist/sdm-core": ">=1.6.0",
    "@atomist/sdm-local": ">=1.0.5"
  },
  "devDependencies": {
    "@atomist/automation-client": "^1.6.0",
    "@atomist/sdm": "1.6.0-master.20190706145121",
    "@atomist/sdm-core": "1.6.0-master.20190708113257",
    "@atomist/sdm-local": "^1.0.7",
    "@types/mocha": "^5.2.6",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3",
    "supervisor": "^0.12.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js{,.map}}\" \"{lib,test}/**/*.{d.ts,js{,.map}}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --config tslint.json --format verbose --project .",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=8.1.0",
    "npm": ">=5.0.0"
  }
}
